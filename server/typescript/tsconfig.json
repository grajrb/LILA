{
  "compilerOptions": {
    "target": "ES5",
    "module": "None",
    "lib": ["ES2018"],
  "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
  "declarationMap": false,
  "sourceMap": false,
  "inlineSourceMap": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "build"]
}